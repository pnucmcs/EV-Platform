{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "App": {
    "Name": "pricing-service",
    "Version": "1.0.0",
    "Environment": "Development"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  },
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Port=5432;Database=pricing_service;Username=postgres;Password=postgres"
  },
  "OpenTelemetry": {
    "Enabled": true,
    "Exporter": "otlp",
    "Otlp": {
      "Endpoint": "http://192.168.1.191:4317"
    },
    "Tracing": {
      "Sampling": "parentbased_always_on"
    }
  }
}
